{
    "buildCommand": "next build",
    "devCommand": "next dev",
    "outputDirectory": ".next",
    "env": {
        "NEXT_PUBLIC_IOS_APP_LINK": "@ios_app_link"
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}